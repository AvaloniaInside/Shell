<TemplatedControl xmlns="https://github.com/avaloniaui"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                  xmlns:views="clr-namespace:AvaloniaInside.Shell.Views"
                  mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
                  x:Class="AvaloniaInside.Shell.Views.ShellView">
	<TemplatedControl.Template>
		<ControlTemplate>
			<Grid>
				<Grid Row="35,*">
					<views:NavigationView Name="PART_NavigationView"/>
					<views:StackContentView VerticalAlignment="Stretch" Name="PART_ContentView"></views:StackContentView>
				</Grid>
			</Grid>
		</ControlTemplate>
	</TemplatedControl.Template>


</TemplatedControl>

