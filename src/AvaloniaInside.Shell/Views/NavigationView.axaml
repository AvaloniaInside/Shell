<TemplatedControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:views="clr-namespace:AvaloniaInside.Shell.Views"
             mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="35"
             x:Class="AvaloniaInside.Shell.Views.NavigationView">
	<TemplatedControl.Template>
		<ControlTemplate>
			<StackLayout Orientation="Horizontal">
				<Button Content="BACK"></Button>
				<views:StackContentView x:Name="Header" HorizontalAlignment="Center" VerticalAlignment="Center" />
			</StackLayout>
		</ControlTemplate>
	</TemplatedControl.Template>
</TemplatedControl>

